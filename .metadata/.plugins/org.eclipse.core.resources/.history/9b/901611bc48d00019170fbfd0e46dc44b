
public class practice10 {

	public static void main(String[] args) {
		// 配列
		
		String[][] country= new String[3][2];
			
			country[0][0]="日本";
			country[0][1]="タイ";
			country[1][0]="アメリカ";
			country[1][1]="ブラシル";
			country[2][0]="フランス";
			country[2][1]="ロシア";
			
		System.out.println(country[0][0]);
			
	}

}
